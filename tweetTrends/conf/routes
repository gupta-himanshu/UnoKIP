# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page


GET   	/           controllers.Application.sessions
GET		/start		controllers.Application.startstream
GET		/test		controllers.Application.testAnalysis(topicId:String)
GET		/testHashtag  controllers.Application.otherAnalysis(topicId:String)
GET		/contactus		controllers.Application.contact
# Map static resources from the /public folder to the /assets URL path
GET   /assets/*file    controllers.Assets.at(path="/public", file)
GET   /webjars/*file   controllers.WebJarAssets.at(file)
